@import 'nib'
@import '../../node_modules/jeet/jeet.styl'
@import '_mixins.styl'

// Media queries
$xl = 1600px
$lg = 1260px
$md = 1024px
$sm = 767px

// Colors
$primary ?= #f7f7f7
$secondary ?= #000
$lightgray ?= transparentize($primary, 0.5)

// Fonts
$font-size = 1rem
title-font()
	font-size 2.5rem
	line-height 1.2
	+below($md)
		font-size 2rem

// Metrics
$padding = ($font-size*2)
$headerSize = ($padding*2 + $font-size)

$kinetic = cubic-bezier(0.77, 0, 0.175, 1)

// CSS Reset
global-reset()

//webfont('TheinhardtReg', 'TheinhardtReg', true, 'normal')
@font-face {
    font-family: 'TheinhardtRegular';
    src: url('../fonts/TheinhardtRegular-Regular.eot');
    src: url('../fonts/TheinhardtRegular-Regular.eot?#iefix') format('embedded-opentype'),
         url('data:application/font-woff;charset=utf-8;base64,') format('woff'),
         url('../fonts/TheinhardtRegular-Regular.svg#TheinhardtRegular') format('svg');
    font-style: normal;
    font-weight: normal;
    text-rendering: optimizeLegibility;
}

body,html
	width 100%
	-webkit-text-size-adjust 100%
	background $primary

html
	font-size 62.5%

body
	font-family 'TheinhardtRegular', Helvetica Neue, Arial, sans-serif
	font-size $font-size
	text-transform uppercase
	font-smoothing()

p
	margin-bottom 1em
	line-height 1
	&:last-child
		margin-bottom 0

.no-margin
	margin 0

.project-infos, #main-informations
	line-height 1.4
	p
		line-height 1.4

::selection
	color $primary
	background $secondary
	
::-moz-selection
	color $primary
	background $secondary

*
	box-sizing(border-box)

video::-webkit-media-controls
	display none

video::-webkit-media-controls-play-button {}

video::-webkit-media-controls-volume-slider {}

video::-webkit-media-controls-mute-button {}

video::-webkit-media-controls-timeline {}

video::-webkit-media-controls-current-time-display {}

a, a:hover, a:visited, a:focus
	color inherit
	text-decoration none
	cursor pointer

#outdated
	display none
	text-align center
	position fixed
	width 100%
	height 100%
	top 0
	left 0
	background $primary
	z-index 900
	a
		text-decoration underline
		text-decoration-skip Ink
	.inner
		padding 8rem 20%

.no-js, .no-requestanimationframe, .no-csstransforms, .no-csstransitions, .no-flexbox
	#outdated
		display block

.lazyimg, .plyr
	transition opacity 300ms ease
	opacity 0

.lazyimg.lazyloaded, .lazyimg.flickity-lazyloaded, .plyr.plyr--ready
	opacity 1

.ajax-loading
	display none
	span(1)
	text-align center
	display none
	h2
		padding $padding 0

#container
	span(1)
	min-height 'calc(100vh - %s)' % ($headerSize*2)

body.project
	#container
		min-height 100vh

header
	hideselect()
	display flex
	justify-content flex-start
	width 100%
	position fixed
	top 0
	left 0
	padding 0 $padding
	white-space nowrap
	background $primary
	border-bottom 1px solid
	z-index 100
	a
		display block
		padding $padding 0 ($padding - 0.1)
		&.active
			span
				border-bottom 1px solid
	#site-title
		flex 1 0
	#filters
		+below($sm)
			display none
		a
			display inline-block
			&:not(:last-child)
				margin-right 0.5em
	#information
		flex 1 0
		text-align right
		+below($sm)
			display none
		a
			display inline-block
			&:not(:last-child)
				margin-right 0.5em
	#menu
		display none
		padding $padding 0 ($padding - 0.1)
		+below($sm)
			display block
		&::after
			content 'Menu'
		&.open::after
			content 'Close'

#mobile-menu
	hideselect()
	position fixed
	width 100%
	top 0
	margin-top 5rem
	background $primary
	z-index 99
	display flex
	flex-wrap wrap
	justify-content space-between
	transition transform 200ms $kinetic, visibility 200ms $kinetic
	transform translateY(-100%) translateZ(0)
	visibility hidden
	+above($md + 1)
		display none
	&.visible
		transform translateY(0%) translateZ(0)
		visibility visible
	a
		display block
		flex 1 0 100%
		padding 1.3em $padding
		text-align center
		border-bottom 1px solid
		position relative
		&.active
			&::after
				content ''
				position absolute
				top 1.1em
				left $padding
				right $padding
				height 1.4em
				border-left 1px solid
				border-right 1px solid
		// &:first-child
		// 	flex 1 0 100%
		// 	text-align center
		// &:nth-child(3n + 2)
		// 	text-align right
		
		

#page-content
	span(1)
	margin-top $headerSize
	margin-bottom -1px
	&::before, &::after
		display none

#main-informations
	hideselect()
	display flex
	padding ($padding / 2) $padding
	border-bottom 1px solid
	+below($md)
		display none
	.info-section
		flex 1 1
		padding 0 $padding
		border-right 1px solid
		&:first-child
			padding-left 0
		&:last-child
			padding-right 0
			border none

#search
	span(1)
	border-bottom 1px solid
	input
		width 50%
		margin-left 25%
		padding $padding
		text-align center
		background none
		border none
		text-transform uppercase
		+below($sm)
			width 100%
			margin-left 0
		&:focus
			outline none
		&::placeholder
			color transparentize($secondary, 0.5)

#projects, #featured
	hideselect()
	span(1)
	.project-item, .featured-item
		span(1/4)
		margin 0 $padding
		border-top 1px solid
		position relative
		+above($md + 1)
			width 'calc(25% - %s)' % ($padding * 2)
			height 20vw
		+below($md)
			height auto
			width 'calc(100% - %s)' % ($padding * 2)
			margin-bottom $padding
			border-bottom none
		&.blank
			+below($md)
				display none !important
		&::before
			content ''
			position absolute
			width 1px
			height calc(100% + 3px)
			background $secondary
			top 0
			right (-1 * $padding)
			+below($md)
				display none
		&:nth-last-child(-n+4)
			&::before
				top initial
				bottom $padding
		a
			display block
			position relative
			width 100%
			height 100%
			&.video
				.project-video
					+below($md)
						display none
				.project-image
					background $primary
				&.play-hover
					.project-image::after
						content ''
						position absolute
						width 100%
						height 100%
						top 0
						left 0
						background url(../../images/play.png) 50% calc(50% + 1rem) no-repeat
						background-size 10%
						+below($md)
							background-position center
				&.play-hover:hover
					.project-image
						+above($md + 1)
							visibility hidden
		.project-infos
			position relative
			z-index 10
			padding $padding 0
			h3
				title-font()
		.project-image, .project-video
			width 100%
			position relative
			+above($md + 1)
				height 100%
				padding ($padding*2 + $font-size*2) $padding ($padding*2)
				position absolute
				top 0
				left 0
			&.featured
				+above($md + 1)
					padding ($padding*5 + $font-size*2) $padding ($padding*5)
			img, video
				display block
				width 100%
				+above($md + 1)
					height 100%
					object-fit contain
	&.simple-grid
		.project-item
			&:first-child
				border-top 0
			&:nth-child(-n+4)
				+above($md + 1)
					border-top 0
				&::before
					top $padding
			&:nth-child(4n)
				&::before
					display none
	&.featured-grid
		.project-item, .featured-item
			&:first-child
				border-top 0
			&:nth-child(-n+3)
				+above($md + 1)
					border-top 0
			&:nth-child(-n+3), &:nth-child(n+7):nth-child(-n+10)
				&::before
					top $padding
			&:nth-child(n+4):nth-child(-n+6)
				&::before
					top initial
					bottom $padding
			&:nth-child(-n+6)
				+above($md + 1)
					width 'calc(33.3333333% - %s)' % ($padding * 2)
					height 25vw
				+below($md)
					width 'calc(100% - %s)' % ($padding * 2)
					height auto
				&:nth-child(3n)
					&::before
						display none
			&:nth-child(n+6)
				&:nth-child(4n + 6)
					&::before
						display none

#featured
	span(1)
	border-bottom 1px solid
	.featured-item
		border none
		+above($md + 1)
			width 'calc(50% - %s)' % ($padding * 2)
			height 50vw
		+below($md)
			height auto
			width 'calc(100% - %s)' % ($padding * 2)
		&::before
			height 'calc(100% - %s)' % ($padding * 2)
		&:nth-child(2)
			&::before
				display none
		a.video
			.project-image::after
					background-position 50% 50%

#page-content.project
	hideselect()
	padding 0
	#project-content
		cursor none
		width 100%
		background $primary
		border-bottom 1px solid
		z-index 50
		top $headerSize
		position fixed
		+above($md + 1)
			height 'calc(100vh - %s - 1px)' % $headerSize
			min-height 300px
		+below($md)
			height 'calc(100vh - %s - %s)' % ($headerSize $padding)
			min-height 250px
	.slider
		height 100%
		.flickity-slider
			transform(none !important)
		.slide
			height 100%
			width 100%
			display inline-flex
			justify-content center
			left 0 !important
			visibility hidden
			.content
				height 100%
				display flex
				justify-content center
				align-items center
				+above($md + 1)
					padding $headerSize $padding ($headerSize * 2)
				+below($md)
					padding $padding $padding ($headerSize * 2 + $padding)
				img
					width 100%
					height 100%
					object-fit contain
			.content.video
				width 100%
				.plyr--ready
					width 100%
					height 100%
				video
					width 100%
					height 100%
					object-fit contain
		.slide.is-selected
			visibility visible
			z-index 1
		.flickity-prev-next-button
			cursor none !important
			top 0
			height 100%
			width 50%
			margin-top 0
			background transparent
			padding 0
			transform(none)
			border none
			border-radius 0
			z-index 10
			+below($md)
				display none
			svg
				display none
			&.previous
				left 0
			&.next
				right 0
			&[disabled]
				display none
		#mouse-nav
			align()
			position absolute
			top -100%
			left -100%
			color $secondary
			z-index 300
			white-space nowrap
			cursor none
			pointer-events none
			display none
			background $primary
			padding 3px 6px 2px
			&::before
				pointer-events none
		&[data-media="video"]:not(.nav-hover)
			#mouse-nav
				&::before
					content 'Play'
			&.pause
				#mouse-nav::before
					content 'Pause'
			&.loading
				#mouse-nav::before
					content 'Loading'
		&[data-media="video"]
			.flickity-prev-next-button
				width 15%
			.plyr__controls
				cursor auto
				+below($md)
					display none
			.plyr--stopped
				.plyr__controls
					opacity 0
		&[data-media="image"], &.nav-hover
			&.left
				#mouse-nav::before
					content 'Prev'
			&.right
				#mouse-nav::before
					content 'Next'
		&:hover
			#mouse-nav
				+above($md)
					display block
	#bottom-bar
		cursor auto
		width 100%
		position absolute
		bottom 0
		left 0
		z-index 10
		padding $padding
		display inline-flex
		justify-content space-between
		line-height 1.4
		[data-target="back"]
			+below($md)
				order 1
		.project-infos
			display inline-flex
			justify-content center
			flex-wrap wrap
			+below($md)
				justify-content flex-start
				padding-right 1em
			#slide-number, .project-title, .project-description
				+below($md)
					width 100%
			.project-title
				+below($md)
					//margin-bottom 1em
			div:not(:last-child)
				+above($md + 1)
					margin-right 1em
			div.additional
				display inline-flex
				justify-content center
				flex-wrap wrap
				+below($md)
					justify-content flex-start
					text-align left
				p
					margin-bottom 0
					&:not(:last-child)
						+above($md + 1)
							margin-right 1em
					+below($md)
						width 100%
						padding-right 1em
		[event-target="more"]
			+below($md)
				display none
	#projects
		margin-top 'calc(100vh - %s)' % $headerSize
		padding $headerSize $padding

#page-content.page
	hideselect()
	position relative
	max-width 100vw
	overflow hidden
	.separator
		+above($sm + 1)
			position absolute
			width 1px
			height 'calc(100% - %s)' % ($padding*2)
			background $secondary
			top $padding
			right 50%
		+below($sm)
			display none
	p
		title-font()
	h1, h2, h3
		margin-bottom 1rem
	.column
		span(1/2)
		width 'calc(50% - %s)' % ($padding * 2)
		margin 0 $padding
		padding $padding 0
		position relative
		+above($sm + 1)
			min-height 'calc(100vh - %s)' % ($headerSize*2)
		+below($sm)
			stack()
			padding $padding
			margin 0
			min-height initial
		&[data-scroll]
			+above($sm + 1)
				height 'calc(100vh - %s)' % ($headerSize*3)
				overflow hidden
			.scroller
				+above($sm + 1)
					padding-bottom 3rem
		&.left
			+below($sm)
				border-bottom 1px solid

footer
	hideselect()
	span(1)
	position relative
	z-index 100
	display inline-flex
	justify-content center
	padding $padding
	background $primary
	border-top 1px solid
	text-align center
	+below($md)
		flex-direction column
	div:not(:last-child)
		margin-right 1em

body.is-loading
	#page-content
		opacity 0

body.landing
	overflow hidden